CREATE TABLE IF NOT EXISTS dev_bd_pilot.bdpilot_technical_metadata (
    ENTITY_NAME STRING,
    ENTITY_COMMENT STRING,
    FIELD_NAME STRING,
    FIELD_TYPE STRING,
    FIELD_COMMENT STRING,
    FIELD_LENGTH STRING,
    FIELD_PRECISION STRING,
    FIELD_FORMAT STRING,
    DATE_MODIFIED STRING,
) ROW FORMAT DELIMITED FIELDS TERMINATED BY \"\\t\" LINES TERMINATED BY \"\\n\" STORED AS TEXTFILE;