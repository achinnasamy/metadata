CREATE TABLE IF NOT EXISTS dev_bd_pilot.bdpilot_operational_data(
    op_type STRING,
    op_name STRING,
    process_id STRING,
    op_start_time_stamp STRING,
    op_end_time_stamp STRING,
    op_status STRING,
    op_owner STRING,
    op_parent_process_name STRING,
    src_entity_name STRING,
    src_record_count STRING,
    target_entity_name STRING,
    target_record_count STRING
)
COMMENT 'Metadata Data'
ROW FORMAT DELIMITED
FIELDS TERMINATED BY '\t'
LINES TERMINATED BY '\n'
STORED AS TEXTFILE;