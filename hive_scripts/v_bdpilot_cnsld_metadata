#Technical, Business and Operational metadata
CREATE TABLE IF NOT EXISTS dev_bd_pilot.v_bdpilot_cnsld_metadata (
    BUSINESS_UNIT STRING,
    ENTITY_NAME STRING,
    ENTITY_BUSINESS_DEFINITION STRING,
    FIELD_NAME STRING,
    FIELD_TYPE STRING,
    FIELD_BUSINESS_DEFINITION STRING,
    OP_TYPE STRING,
    OP_NAME STRING
) ROW FORMAT DELIMITED FIELDS TERMINATED BY \"\\t\" LINES TERMINATED BY \"\\n\" STORED AS TEXTFILE;